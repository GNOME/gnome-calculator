                <style>
                  <class name="clear-button"/>
                </style>
                <layout>
                  <property name="column">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="calc_start_group_button">
                <property name="label">&#x200E;(</property>
